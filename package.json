{
  "name": "openai-advanced",
  "version": "1.0.0",
  "description": "A TypeScript library for advanced usage of OpenAI APIs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "docs": "typedoc --out docs src",
    "build-and-run": "npm run prepare && npm run docs && node dist/sanity.js"
  },
  "keywords": ["OpenAI", "AI", "TypeScript", "library", "machine-learning", "deep-learning", "chatbot", "API"],
  "author": "Yaron Gilo Yarongilo@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yarongilo1981/openai-advanced.git"
  },
  "bugs": {
    "url": "https://github.com/yarongilo1981/openai-advanced/issues"
  },
  "homepage": "https://github.com/yarongilo1981/openai-advanced#readme",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "json-schema-service": "^1.0.3",
    "openai": "^4.59.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  }
}
